<configuration>
  <system.webServer>
	<!-- Indica que todos los archivos .js son aplicaciones node.js y tienen que ser tratadas por el mÃ³dule de iisnode -->
	<handlers>
		<add name="iisnode" path="*.js" verb="*" modules="iisnode" />
	</handlers>
	<rewrite>
		<rules>
			<rule name="app">
				<match url="api/" />
				<action type="Rewrite" url="app.js" />
			</rule>
		</rules>
	</rewrite>
  </system.webServer>
</configuration>
